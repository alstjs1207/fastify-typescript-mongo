{
  "name": "fastify-typescript-mongo",
  "version": "0.0.1",
  "description": "fastify-typescript-mongo template",
  "main": "index.js",
  "scripts": {
    "test": "jest ./build/api/v0/*/*.spec.js --detectOpenHandles --forceExit",
    "test:server": "jest ./build/*.spec.js --detectOpenHandles --forceExit",
    "prebuild": "npm-run-all clean lint",
    "build": "npx tsc --version && tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf ./build",
    "clean-all": "rimraf ./build ./node_modules",
    "dev": "nodemon build/index.js",
    "start": "node build/index.js",
    "lint": "eslint .",
    "ci:test": "NODE_ENV=test npm-run-all build test"
  },
  "_moduleAliases": {
    "@api": "build/api",
    "@server": "build/server.js",
    "@lib": "build/lib",
    "@policy": "build/policy/index.js",
    "@service": "build/service",
    "@dao": "build/dao",
    "@schema": "build/schema/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alstjs1207/fastify-typescript-mongo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alstjs1207/fastify-typescript-mongo/issues"
  },
  "homepage": "https://github.com/alstjs1207/fastify-typescript-mongo#readme",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/mongodb": "^7.0.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.5.0",
    "fastify": "^4.14.1",
    "fastify-plugin": "^4.5.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.1.0",
    "nodemon": "^2.0.20",
    "pino": "^8.8.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-runner": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
